def pods
  pod 'Alamofire', '~> 5.0.0-beta.5'
end

target 'DataTrend' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DataTrend
  pods
  
  target 'DataTrendTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DataTrendUITests' do
    inherit! :search_paths
    # Pods for testing
    pods
  end

end
